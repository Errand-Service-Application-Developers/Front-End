{
  "expo": {
    "name": "ErrandGo",
    "slug": "ErrandGo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./apps/assets/appicon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./apps/assets/splashScreen.png",
      "resizeMode": "cover",
      "backgroundColor": "#e5525f"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./apps/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./apps/assets/favicon.png"
    }
  }
}
